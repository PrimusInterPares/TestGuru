<tr>
  <td><%= test.title %></td>
  <td class="text-center"><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td class="text-center"><%= test.questions.size %></td>
  <td><%= link_to t('.show'), admin_test_path(test), class: "btn btn-outline-dark text-dark" %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test), class: "btn btn-outline-dark text-dark" %></td>
  <td>
    <%= link_to t('.delete'),
                admin_test_path(test),
                  data: {
                    turbo_method: :delete,
                    turbo_confirm: t('.confirm')
                  },
                class: "btn btn-danger"
    %>
  </td>
</tr>